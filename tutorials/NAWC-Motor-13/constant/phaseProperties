FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type            regressionMultiphaseSystem;

phases          (gas particles propellant);

particlePhase	particles;

gasPhase	gas;

propellantIndex 2;

Xp      0;

Tad                 2850;

Tset		    2000;

propellantRho       1700;

gas
{
    type            pureThermalEnergyPhaseModel;
    diameterModel   constant;
    constantCoeffs
    {
        d               2e-6;
    }
    residualAlpha   1e-15;
}

particles
{
    type            pureParticlePhaseModel;
    diameterModel   constant;
    constantCoeffs
    {
        d               2e-6;
    }
    residualAlpha   1e-15;
}

propellant
{
    type            pureIsothermalStaticPhaseModel;
    diameterModel   constant;
    constantCoeffs
    {
        d           2e-6;
    }
    residualAlpha   1e-15;
}

interfaceTracking
(
  (particles in gas)
  {
    type            subCellularInterfaceMotion;
    propellant      propellant;
    n               0.461;
    a               0.222;
    f               1.0;
    Pmin	          10;
  }
);

sharpInterfaceHeatTransfer
(
  (particles in gas)
  {
    type            RanzMarshall;
    residualAlpha   1e-15;
    cutoff          1e-15;
    gammaR          397;
  }
);

drag
(
  (particles in gas)
  {
    type            SchillerNaumann;
    residualRe      1e-15;
    R	 	    327;
    gamma	    1.21;
    swarmCorrection
    {
    	type        none;
    }
  }
);

blending
{
    default
    {
      type            none;
      continuousPhase gas;
    }
}

surfaceTension
();

aspectRatio
();

virtualMass
();

interfaceComposition
();

interfaceCompression
();

// Models used to find Mass Transfer Coefficient for component transfer within the gas phase
massTransfer.gas
();

massTransfer.propellant
();

heatTransfer.particles
();

heatTransfer.gas
();

heatTransfer.propellant
();

phaseTransfer
();

lift
();

wallLubrication
();

turbulentDispersion
();

// Minimum allowable pressure
pMin            10;


